<!DOCTYPE html>
<html>

<head>
  <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <meta property="og:site_name" content="Viblo" />
  <meta property="og:title" content="{{ htmlWebpackPlugin.options.title }}" />
  <meta property="og:description" content="" />
  <meta property="og:url" content="https://viblo.asia/careers" />
  <meta property="og:image" content="~/assets/og-image.png" />
  <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,700&amp;subset=vietnamese" rel="stylesheet">
  <title>{{ htmlWebpackPlugin.options.title }}</title>
</head>

<body>
  <div id='site'>
    {{> ./parts/top }}

    {{> ./parts/values }}

    {{> ./parts/jobs }}

    {{> ./parts/footer }}
  </div>

  {{#if htmlWebpackPlugin.options.googleAnalytics}}
  <script type="text/javascript">
    window.GoogleAnalyticsObject = 'ga'; window.ga = function () { ga.q.push(arguments) }; ga.q = []; ga.l = +new Date;
    ga('create', '{{ htmlWebpackPlugin.options.googleAnalytics.trackingId }}', 'auto');
    ga('send', 'pageview');
  </script>
  {{/if}}
  <script async defer src="https://www.google-analytics.com/analytics.js" type="text/javascript"></script>
</body>

</html>
